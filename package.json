  },
  "dependencies": {},
  "devDependencies": {
    "laravel-mix": "^4.1.4",
    "laravel-mix-purgecss": "^4.1.0",
    "tailwindcss": "^1.1.4",
    "vue-template-compiler": "^2.6.10"
  }
}
